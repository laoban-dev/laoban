{
  "template":    "typescript",
  "name":        "laoban",
  "description": "A cli for managing projects that have many npm packages",
  "packageJson": {
    "bin":             {
      "laoban": "dist/index.js"
    },
    "dependencies":    {
      "fs-extra":  "^9.0.1",
      "commander": "^6.2.0"
    },
    "devDependencies": {
      "@types/fs-extra": "^9.0.5"
    },
    "keywords":        [
      "monorepo",
      "devops"
    ]
  },
  "details":     {
    "publish":      true,
    "test":         true,
    "links":        [
      "@laoban/variables",
      "@laoban/generations",
      "@laoban/validation",
      "@laoban/debug",
      "@laoban/node",
      "@laoban/filesops-node"
    ],
    "compile":      true,
    "extraDeps":    {
      "fs-extra":  "^9.0.1",
      "commander": "^6.2.0"
    },
    "extraDevDeps": {
      "@types/fs-extra": "^9.0.5"
    },
    "extraBins":    {
      "laoban": "dist/index.js"
    }
  }
}