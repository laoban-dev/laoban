{
  "markers":              ["json:packageJson.devDependencies.typescript"],
  "parents":              [
    "@laoban@/init/javascript_core"
  ],
  "laoban.json":          {
    "packageManager": "yarn",
    "parents":        [
      "@laoban@/laoban.json/typescript.laoban.json",
      "@laoban@/laoban.json/typescript.publish.laoban.json"
    ]
  },
  "package.details.json": {
    "guardfile":     "project.json",
    "variableFiles": {"packageJson": "project.json"},
    "contents":      {
      "template":    "typescript",
      "name":        "${packageJson.name}",
      "description": "${packageJson.description}",
      "details":     {
        "publish": false,
        "compile":     true,
        "test":    true,
        "links":   []
      }
    }
  }
}
